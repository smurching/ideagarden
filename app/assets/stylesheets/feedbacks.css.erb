/* feedback */

.feedback {
  clear: both;
}

.feedback_content {
  float: left;
  margin-left: 10px;
}

/* feedback help rating */

.feedback_help * {
  float: left; 
}

/* highlighting for followed users' feedback */

.followed_feedback {
  margin-left: 10px;
  background-color: rgb(0,255,0);
  height: 30px;
}
